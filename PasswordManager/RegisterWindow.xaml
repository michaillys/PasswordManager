<Window x:Class="PasswordManager.RegisterWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PasswordManager"
        mc:Ignorable="d"
        Title="Register" Height="200" Width="400">
    <Grid>
        <StackPanel>
            <TextBlock Text="UserName:" Margin="10 10 0 0"/>
            <TextBox x:Name="UsernameTextBox" Margin="10 0 10 0"/>
            <TextBlock Text="Password:" Margin="10 10 0 0"/>
            <PasswordBox x:Name="PasswordBox" Margin="10 0 10 0"/>
            <Button Content="Register" Click="RegisterButton_Click" Margin="10"/>
        </StackPanel>
    </Grid>
</Window>
